{
    "name": "tfsthiagobr98/laravel-keycloak",
    "description": "Simple Keycloak Integration for Laravel",
    "homepage": "https://github.com/TFSThiagoBR98/laravel-keycloak-web-guard",
    "license": "MIT",
    "keywords": [
        "laravel",
        "keycloak",
        "jwt",
        "authentication"
    ],
    "authors": [
        {
            "name": "Thiago França",
            "email": "packages@tfs.dev.br"
        },
        {
            "name": "Robson Tenório"
        },
        {
            "name": "Mário Valney",
            "email": "mariovalney@gmail.com"
        }
    ],
    "minimum-stability": "stable",
    "require": {
        "guzzlehttp/guzzle": "^6.5|^7.0",
        "illuminate/auth": "^9.0|^10.0",
        "illuminate/http": "^9.0|^10.0",
        "illuminate/support": "^9.0|^10.0",
        "illuminate/routing": "^9.0|^10.0",
        "illuminate/contracts": "^9.0|^10.0",
        "firebase/php-jwt": "^6.4",
        "spatie/laravel-schemaless-attributes": "^2.4"
    },
    "require-dev": {
        "mockery/mockery": "^1.5",
        "orchestra/testbench": "^7.0|^8.0",
        "phpstan/phpstan": "^1.10",
        "phpunit/phpunit": "^10.0",
        "nunomaduro/collision": "^7.0",
        "nunomaduro/larastan": "^2.4",
        "phpstan/phpstan-phpunit": "^1.3",
        "friendsofphp/php-cs-fixer": "^3.14"
    },
    "autoload": {
        "psr-4": {
            "TFSThiagoBR98\\LaravelKeycloak\\": "src/"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "TFSThiagoBR98\\LaravelKeycloak\\Tests\\": "tests/"
        }
    },
    "extra": {
        "laravel": {
            "providers": [
                "TFSThiagoBR98\\LaravelKeycloak\\LaravelKeycloakServiceProvider"
            ],
            "aliases": {
                "KeycloakWeb": "TFSThiagoBR98\\LaravelKeycloak\\Facades\\KeycloakWeb"
            }
        }
    }
}
